{
  "expo": {
    "name": "Sif player",
    "slug": "sif-player",
    "version": "1.0.0",
    "orientation": "default",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
