    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pengidev.sif_player"
    },
    "owner": "pengi-dev",
    "extra": {
      "eas": {
        "projectId": "7204efa3-0b46-4927-9212-6a3faddbab94"
      }
    }
  }
}
